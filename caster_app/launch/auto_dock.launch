<?xml version="1.0"?>
<launch>
  <arg name="log_type" default="screen"/>

  <node pkg="caster_app" type="dock_server_v2.py" name="dock_server_node" output="$(arg log_type)">
    <rosparam file="$(find caster_app)/config/auto_dock.yaml" command="load" />
    <remap from="caster_app_node/base_scan" to="scan" />
    <remap from="cmd_vel" to="yocs_cmd_vel_mux/input/navigation_cmd" />
  </node>

  <node pkg="caster_app" type="dock_v2_detect.py" name="dock_detect_node" output="$(arg log_type)">
    <rosparam file="$(find caster_app)/config/auto_dock.yaml" command="load" />
  </node>
</launch>